{% capture CheckOut %}
  {% liquid
    case section.settings.VEDA_radioGroup_2
      when 'left'
        assign positionCheckout = 'jc:flex-start'
      when 'center'
        assign positionCheckout = 'jc:center'
      else
        assign positionCheckout = 'jc:flex-end'
    endcase
  %}
  <div class="cart-checkout-js w:100% d:flex {{ positionCheckout }}">
    <div class="w:100% maw:400px@400px">
      <div class="w:100% h:70px d:flex mt:20px bdb:1px_solid_color-gray3">
        {% for block in section.blocks %}
          {% assign actionItem = block.settings %}

          {% if actionItem.actions___enable %}
            {% case block.type %}
              {% when 'actions_note' %}

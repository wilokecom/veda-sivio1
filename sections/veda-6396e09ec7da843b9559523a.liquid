                  class="mini-cart-btn-note-js d:flex fld:column jc:center ai:center flxs:1 w:100% h:100% fz:pfs(13px,15px) c:color-gray9 bdr:1px_solid_color-gray3 bd:none!|lc cur:pointer bdrs:4px||be fz:13px||be"
                  data-tooltip-position="top"
                  data-tooltip-text="{{ 'veda.sections.cart.special_instrutions_for_seller' | t }}"
                  data-css="{{ actionItem.actions___design__id }}"
                >
                  <span class="fz:18px" data-css="{{ actionItem.VEDA_styleBox_26 }}">
                    <i class="far fa-pencil-alt"></i>
                  </span>
                  <span class="ff:font-secondary">{{ 'veda.sections.cart.note_title' | t }}</span>
                </div>
              {% when 'actions_shipping' %}
                <div
                  class="mini-cart-btn-shipping-js d:flex fld:column jc:center ai:center flxs:1 w:100% h:100% fz:pfs(13px,15px) c:color-gray9 bdr:1px_solid_color-gray3 bd:none!|lc cur:pointer bdrs:4px||be fz:13px||be"
                  data-tooltip-position="top"
                  data-tooltip-text="{{ 'veda.sections.cart.estimate_shipping_rates' | t }}"
                  data-css="{{ actionItem.actions___design__id }}"
                >
                  <span class="fz:18px" data-css="{{ actionItem.VEDA_styleBox_26 }}">
                    <i class="far fa-shipping-fast"></i>
                  </span>
                  <span class="ff:font-secondary">{{ 'veda.sections.cart.shipping_title' | t }}</span>
                </div>
              {% when 'actions_discount' %}
                <div
                  class="mini-cart-btn-discount-js d:flex fld:column jc:center ai:center flxs:1 w:100% h:100% fz:pfs(13px,15px) c:color-gray9 bdr:1px_solid_color-gray3 bd:none!|lc cur:pointer bdrs:4px||be fz:13px||be"
                  data-css="{{ actionItem.actions___design__id }}"
                  data-tooltip-position="top"
                  data-tooltip-text="{{ 'veda.sections.cart.add_discount_code' | t }}"
                >
                  <span class="fz:18px" data-css="{{ actionItem.VEDA_styleBox_26 }}">
                    <i class="far fa-percent"></i>
                  </span>
                  <span class="ff:font-secondary">{{ 'veda.sections.cart.coupon_code' | t }}</span>
                </div>
            {% endcase %}
          {% endif %}
        {% endfor %}
      </div>
      {{ Note }}
      {{ Shipping }}
      {{ Discount }}

      {% for block in section.blocks %}
        {% assign checkOutItem = block.settings %}

        {% case block.type %}
          {% when 'check_out_price' %}
            <div class="d:flex ai:center jc:space-between">
              <div class="mt:20px fz:15px ff:font-secondary fw:500" data-css="{{ checkOutItem.VEDA_styleBox_42 }}">
                {{ 'veda.sections.cart.subtotal' | t }}
              </div>
              <span
                class="mt:20px fz:20px ff:font-secondary fw:500"
                data-css="{{ checkOutItem.VEDA_styleBox_44 }}"
              >
                {{ cart.total_price | money }}
              </span>
            </div>
            <div>
              {% if cart.cart_level_discount_applications.size > 0 %}
                <ul class="p:0! ls:none" aria-label="{{ 'veda.customer.order.discount' | t }}">
                  {% for discountItem in cart.cart_level_discount_applications %}
                    <li class="mt:5px fz:15px ff:font-secondary" data-css="{{ discountItem.discount_design.id }}">
                      {{ discountItem.title }}(-{{ discountItem.total_allocated_amount | money }})
                    </li>
                  {% endfor %}
                </ul>
              {% endif %}
            </div>
          {% when 'check_out_text' %}
            <div class="mt:10px fz:14px c:color-gray7" data-css="{{ checkOutItem.VEDA_styleBox_48 }}">
              {% if cart.taxes_included and shop.shipping_policy.body != blank %}
                {{ 'veda.sections.cart.taxes_included_and_shipping_policy_html' | t: link: shop.shipping_policy.url }}
              {% elsif cart.taxes_included %}
                {{ 'veda.sections.cart.taxes_included_but_shipping_at_checkout' | t }}
              {% elsif shop.shipping_policy.body != blank %}
                {{
                  'veda.sections.cart.taxes_and_shipping_policy_at_checkout_html'
                  | t: link: shop.shipping_policy.url
                }}
              {% else %}
                {{ 'veda.sections.cart.taxes_and_shipping_at_checkout' | t }}
              {% endif %}
            </div>
          {% when 'check_out_button' %}
            {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
            {% liquid
              if checkOutItem.check_out___variant == 'link'
                assign default_class = 'rs lh:1.5 fz:15px ff:font-primary fw:500 bd:0 bxsh:none p:0 bg:none cur:pointer trs:0.3s '
              else
                assign default_class = 'rs lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s p:13px_20px bd:none bxsh:none o:none cur:pointer '
              endif

              assign default_for_bottom_class = ''

              case checkOutItem.check_out___variant
                when 'primary'
                  assign variant_class = ' bgc:color-primary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                when 'secondary'
                  assign variant_class = ' bgc:color-secondary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                when 'dark'
                  assign variant_class = ' bgc:color-gray9 c:color-light bgc:color-primary|h c:color-light-freeze|h'
                when 'gray'
                  assign variant_class = ' bgc:color-gray2 c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                when 'light'
                  assign variant_class = ' bgc:color-light c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                else
                  assign variant_class = ' c:color-gray9 c:color-primary|h'
              endcase
            %}

            <button
              type="{{ 'submit' }}"
              component="{{ 'check_out' }}"
              {% if 'w:100% mt:20px' %}
                class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }} {{ 'w:100% mt:20px' }}"
              {% else %}
                class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }}"
              {% endif %}
              {% if _data_confirm_message %}
                data-confirm-message="{{ _data_confirm_message }}"
              {% endif %}
              {% if _data_target %}
                data-target="{{ _data_target }}"
              {% endif %}
              {% if _aria_label %}
                aria-label="{{ _aria_label }}"
              {% endif %}
              {% if _data_index %}
                data-index="{{ _data_index }}"
              {% endif %}
              {% if _data_form_active %}
                data-form-active="{{ _data_form_active }}"
              {% endif %}
              {% if 'cart-drawer-form' %}
                form="{{ 'cart-drawer-form' }}"
              {% endif %}
              {% if 'checkout' %}
                name="{{ 'checkout' }}"
              {% endif %}
              {% if 'checkout' %}
                id="{{ 'checkout' }}"
              {% endif %}
              {% if _tooltip_position %}
                data-tooltip-position="{{ _tooltip_position }}"
              {% endif %}
              {% if _tooltip_text %}
                data-tooltip-text="{{ _tooltip_text }}"
              {% endif %}
              {% if _data_button_video_id %}
                data-button-video-id="{{ _data_button_video_id }}"
              {% endif %}
              data-css="{{ checkOutItem.check_out___design__id }}"
            >
              <span>
                {{ 'veda.sections.cart.checkout' | t }}
              </span>
            </button>
            {% comment %}KET_THUC_TACH_FILE{% endcomment %}
          {% else %}

        {% endcase %}
      {% endfor %}
    </div>
  </div>
{% endcapture %}

{% capture Content %}
  <div
    class="col-xs-{{ section.settings["VEDA_responsive_4"] }} col-sm-{{ section.settings["VEDA_responsive_3"] }} col-md-{{ section.settings["VEDA_responsive_2"] }} col-lg-{{ section.settings["VEDA_responsive_1"] }} mt:0!"
    {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
      style="{{ colGap }}"
    {% endif %}
  >
    {% if builderMode %}
      {% if section.settings.testing == 'has_item' %}
        <form action="{{ routes.cart_url }}" method="post" id="cart-drawer-form" class="w:100%">
          {{ TableHeader }}
          {{ TableBody }}
        </form>
        {{ CheckOut }}
      {% else %}
        {{ EmptyContent }}
      {% endif %}
    {% else %}
      {% if cart.item_count == 0 %}
        {{ EmptyContent }}
      {% else %}
        <form action="{{ routes.cart_url }}" method="post" id="cart-drawer-form" class="w:100%">
          {{ TableHeader }}
          {{ TableBody }}
        </form>
        {{ CheckOut }}
      {% endif %}
    {% endif %}
  </div>
{% endcapture %}

<section
  data-id="{{ uniqueId }}"
  class="veda-lazyload pos:relative z:1 bgz:cover bgp:center"
  style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
>
  <div
    class="{{ containerType }}"
    data-id="cartcontainer"
    {% if section.settings.VEDA_slider_1
      or section.settings.VEDA_slider_1 == 0
      or section.settings.section_settings___width
      or section.settings.section_settings___width == 0
    %}
      style="{{ containerGap }}{{ containerWidth }}"
    {% endif %}
  >
    <div
      {% if section.settings.VEDA_radioGroup_1 == 'left' %}
        class="row mt:0!"
      {% else %}
        class="row mt:0! fld:row-reverse"
      {% endif %}
    >
      <div class="col-xs-{{ section.settings["VEDA_responsive_8"] }} col-sm-{{ section.settings["VEDA_responsive_7"] }} col-md-{{ section.settings["VEDA_responsive_6"] }} col-lg-{{ section.settings["VEDA_responsive_5"] }} mt:0!">
        <div
          {% if section.settings.VEDA_responsive_5 == 12 %}
            class="pos:relative d:flex ai:center jc:space-between"
          {% else %}
            class="pos:relative"
          {% endif %}
        >
          {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
          <div class="d:flex fld:column w:100%">
            {% for block in section.blocks %}
              {% assign heading_item = block.settings %}

              {% if heading_item.heading___enable %}
                {% liquid
                  assign align = heading_item.heading___align

                  if heading_item.heading___align == 'left'
                    assign align = 'start'
                  elsif heading_item.heading___align == 'right'
                    assign align = 'end'
                  endif
                %}
                {% capture attr %}
                  data-css="{{ heading_item.VEDA_styleBox_2 }}" class="fw:500" style="text-align: {{ align }}"
                {% endcapture %}
                {% case block.type %}
                  {% when 'heading_title' %}
                    {% if heading_item.heading___title_tag_name == 'h2' %}
                      <h2 {{ attr }}>{{ heading_item.heading___title }}</h2>
                    {% elsif heading_item.heading___title_tag_name == 'h3' %}
                      <h3 {{ attr }}>{{ heading_item.heading___title }}</h3>
                    {% elsif heading_item.heading___title_tag_name == 'h4' %}
                      <h4 {{ attr }}>{{ heading_item.heading___title }}</h4>
                    {% elsif heading_item.heading___title_tag_name == 'h5' %}
                      <h5 {{ attr }}>{{ heading_item.heading___title }}</h5>
                    {% elsif heading_item.heading___title_tag_name == 'h6' %}
                      <h6 {{ attr }}>{{ heading_item.heading___title }}</h6>
                    {% endif %}
                  {% when 'heading_text' %}
                    <div
                      data-css="{{ heading_item.VEDA_styleBox_4 }}"
                      style="text-align: {{ align }}"
                      class="fz:16px"
                    >
                      {{ heading_item.heading___text }}
                    </div>
                  {% when 'heading_icon' %}
                    <div
                      data-css="{{ heading_item.VEDA_styleBox_6 }}"
                      {% if heading_item.heading___align == 'left' %}
                        style="text-align: start"
                      {% elsif heading_item.heading___align == 'center' %}
                        style="text-align: {{ heading_item.heading___align }}; margin-left: auto; margin-right: auto"
                      {% elsif heading_item.heading___align == 'right' %}
                        style="text-align: end; margin-left: auto; margin-right: 0"
                      {% endif %}
                      class="fz:34px"
                    >
                      {{ heading_item.heading___icon }}
                    </div>
                  {% when 'heading_space' %}
                    <div style="height: {{ heading_item.heading___space }}px"></div>
                  {% when 'link' %}
                    <a
                      href="{{ heading_item.link }}"
                      class="ff:font-secondary fz:16px fw:500 c:color-gray9 c:color-gray9|h! td:underline"
                      data-css="{{ heading_item.link_design.id }}"
                      style="text-align: {{ align }}"
                    >
                      {{ heading_item.text_link }}
                    </a>
                  {% when 'heading_divider' %}
                    {% assign headingDividerPadding = 'padding-top: '
                      | append: heading_item.VEDA_space_8
                      | append: 'px;padding-right: '
                      | append: heading_item.VEDA_space_7
                      | append: 'px;padding-bottom: '
                      | append: heading_item.VEDA_space_5
                      | append: 'px;padding-left: '
                      | append: heading_item.VEDA_space_6
                      | append: 'px;'
                    %}
                    <div
                      class="pt:20px fz:0"
                      style="text-align: {{ align }};{{ headingDividerPadding }}"
                    >
                      {% if heading_item.heading___divider == 'wave' %}
                        <div class="d:inline-block">
                          <svg
                            width="56px"
                            height="7px"
                            viewBox="0 0 56 7"
                            version="1.1"
                            xmlns="http://www.w3.org/2000/svg"
                            xmlns:xlink="http://www.w3.org/1999/xlink"
                            fill="none"
                          >
                            <g transform="translate(-288.000000, -4390.000000)" class="stk:color-gray3" style="stroke: {{ heading_item.heading___divider_color }}">
                                <g transform="translate(135.000000, 4033.000000)">
                                    <g transform="translate(0.000000, 48.000000)">
                                        <g transform="translate(153.000000, 310.000000)">
                                            <path d="M0,0 C4.64733427,0 4.65894638,2 9.33333333,2 C14.0077203,2 13.9400367,0 18.6666667,0 C23.3932966,0 23.3301172,2 28,2 C32.6698828,2 32.6447946,0 37.3333333,0 C42.0218721,0 41.9540981,2 46.6666667,2 C51.3792353,2 51.3554302,0 56,0"></path>
                                            <path d="M0,3 C4.64733427,3 4.65894638,5 9.33333333,5 C14.0077203,5 13.9400367,3 18.6666667,3 C23.3932966,3 23.3301172,5 28,5 C32.6698828,5 32.6447946,3 37.3333333,3 C42.0218721,3 41.9540981,5 46.6666667,5 C51.3792353,5 51.3554302,3 56,3"></path>
                                        </g>
                                    </g>
                                </g>
                            </g>
                          </svg>
                        </div>
                      {% elsif heading_item.heading___divider == 'mountain' %}
                        <div class="d:inline-block">
                          <svg
                            width="60px"
                            height="9px"
                            viewBox="0 0 60 9"
                            version="1.1"
                            xmlns="http://www.w3.org/2000/svg"
                            xmlns:xlink="http://www.w3.org/1999/xlink"
                          >
                            <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                                <g transform="translate(-3.000000, -47.000000)" stroke-width="2" class="stk:color-gray3" style="stroke: {{ heading_item.heading___divider_color }}">
                                    <g transform="translate(0.000000, 45.000000)">
                                        <polyline points="4 9 9.8 4 15.6 9 21.4 4 27.2 9 33 4 38.8 9 44.6 4 50.4 9 56.2 4 62 9"></polyline>
                                    </g>
                                </g>
                            </g>
                          </svg>
                        </div>
                      {% else %}
                        <div
                          class="d:inline-block! w:50px h:2px bgc:color-gray3"
                          style="background-color: {{ heading_item.heading___divider_color }}"
                        ></div>
                      {% endif %}
                    </div>
                  {% else %}

                {% endcase %}
              {% endif %}
            {% endfor %}
          </div>
          {% comment %}KET_THUC_TACH_FILE{% endcomment %}
        </div>
      </div>
      {{ Content }}
    </div>
  </div>
  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</section>

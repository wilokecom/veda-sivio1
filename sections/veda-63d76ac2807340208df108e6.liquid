{% assign uniqueId = 'id_84032e04-5a14-4754-8655-a3a0ba33615c' %}
{% assign builderMode = false %}
{% assign envatoMode = false %}
{% liquid
  if section.settings.VEDA_switch_1
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_2 == 'color'
    assign backgroundColor = 'background-color: ' | append: section.settings.VEDA_color_1 | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2
    assign bgImage = section.settings.VEDA_image_2 | append: '&width=2000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  elsif section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2 == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: section.settings.VEDA_space_4 | append: 'px;padding-right: ' | append: section.settings.VEDA_space_3 | append: 'px;padding-bottom: ' | append: section.settings.VEDA_space_1 | append: 'px;padding-left: ' | append: section.settings.VEDA_space_2 | append: 'px;'

  if section.settings.VEDA_select_1 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_1 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_1 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_1 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_2 >= 0
    assign settingGap = section.settings.VEDA_slider_2 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_2
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_3
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _gap = false

  assign _thumbnails_enable = false

  assign _slides_per_view_pagination = false

  assign _enable = false

  assign _class = false

  assign _component = false

  assign _container_class = false

  assign _design = false

  assign _button_class = false

  assign _input_class = false

  assign _name = false

  assign _form = false

  assign _data_confirm_message = false

  assign _data_target = false

  assign _aria_label = false

  assign _data_index = false

  assign _data_form_active = false

  assign _id = false

  assign _tooltip_position = false

  assign _tooltip_text = false

  assign _data_button_video_id = false

  assign _icon_class = false
%}
{% capture Image %}
  {% if product %}
    {% liquid
      assign imageXs = 12 | minus: section.settings.responsive___content__xs

      assign imageSm = 12 | minus: section.settings.responsive___content__sm

      assign imageMd = 12 | minus: section.settings.responsive___content__md

      assign imageLg = 12 | minus: section.settings.responsive___content__lg
    %}
    <div
      class="col-xs-{{ imageXs }} col-sm-{{ imageSm }} col-md-{{ imageMd }} col-lg-{{ imageLg }} d:flex fld:column ai:center pr:30px"
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ colGap }}"
      {% endif %}
    >
      {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
      {% liquid
        assign image_length = product.images | size

        assign variants_length = product.variants | size

        assign gap = 10

        if section.settings.product_media___gap
          assign gap = section.settings.product_media___gap
        endif
      %}
      {% if image_length <= 1 or variants_length <= 1 %}
        <div class="pos:relative w:100% z:1">
          {% liquid
            assign imageItem = product.featured_image

            if imageItem.src != blank
              assign classImage = 'veda-photo-swipe veda-image-zoom w:100% h:auto'
            else
              assign classImage = 'w:100% h:auto'
            endif
          %}
          <div
            class="{{ classImage }}"
            data-image-zoom-src="{{ imageItem.src | image_url: width: imageItem.width }}"
            data-image-zoom="2"
          >
            {% if imageItem.src != blank %}
              <a
                class="d:flex jc:center ai:center pos:absolute t:0 l:0 w:100% h:100% clp:rect(20px,50px,50px,20px) trf:rotateY(180deg) cur:pointer z:999"
                href="{{ imageItem.src | image_url: width: imageItem.width }}"
                data-pswp-width="{{ imageItem.width }}"
                data-pswp-height="{{ imageItem.height }}"
              >
                <span class="w:30px h:30px bgc:color-light pos:absolute t:20px l:20px d:flex jc:center ai:center bdrs:50%">
                  <i class="fal fa-search-plus fz:18px"></i>
                </span>
                <img
                  class="w:100% pos:absolute ins:0 op:0 bgc:color-light-freeze"
                  src="{{ imageItem.src | image_url: width: 10 }}"
                  alt="{{ variantItem.title }}"
                  width="{{ imageItem.width }}"
                  height="{{ imageItem.height }}"
                  data-intrinsic-width="{{ imageItem.width }}"
                  data-intrinsic-height="{{ imageItem.height }}"
                  loading="lazy"
                >
              </a>
            {% endif %}
            <div
              {% if imageItem.src != blank %}
                href="{{ imageItem.src | image_url: width: imageItem.width }}"
              {% endif %}
              class="d:block w:100% cur:zoom-in"
              data-pswp-width="{{ imageItem.width }}"
              data-pswp-height="{{ imageItem.height }}"
            >
              {% if imageItem.src != blank %}
                <img
                  class="veda-lazyload w:100%"
                  data-image-zoom-src="{{ imageItem.src | image_url: width: imageItem.width }}"
                  data-image-zoom="2"
                  src="{{ imageItem.src | image_url: width: 10 }}"
                  alt="{{ variantItem.title }}"
                  width="{{ imageItem.width }}"
                  height="{{ imageItem.height }}"
                  data-intrinsic-width="{{ imageItem.width }}"
                  data-intrinsic-height="{{ imageItem.height }}"
                  loading="lazy"
                >
              {% else %}
                {{ 'image' | placeholder_svg_tag: 'veda-placeholder-svg bgc:color-gray2 ' }}
              {% endif %}
            </div>
          </div>
        </div>
      {% else %}
        <div class="w:100% pos:relative veda-photo-swipe">
          {% liquid
            if section.settings.VEDA_switch_5
              assign image_width = 'calc(100% - 110px);'
            else
              assign image_width = '100%;'
            endif
          %}
          <div
            class="veda-swiper"
            {% if section.settings.VEDA_switch_5 %}
              style="padding-left: {{ gap }}px"
            {% endif %}
            data-options="
              {
                speed: 400,
                centeredSlides: false,
                autoHeight: true,
              }
            "
          >
            <div class="swiper">
              <div class="swiper-wrapper">
                {% for variantItem in product.variants %}
                  {% liquid
                    if variantItem.featured_image.src != blank
                      assign imageItem = variantItem.featured_image
                    elsif variantItem.image.src != blank
                      assign imageItem = variantItem.image
                    else
                      assign imageItem = product.images[forloop.index0]
                    endif

                    if imageItem.src != blank
                      assign classImage = 'swiper-slide veda-image-zoom w:100% h:100%'
                    else
                      assign classImage = 'swiper-slide w:100% h:100%'
                    endif
                  %}
                  <div
                    class="{{ classImage }} h:100% pos:relative"
                    data-image-zoom-src="{{ imageItem.src | image_url: width: imageItem.width }}"
                    data-image-zoom="2"
                  >
                    {% if imageItem.src != blank %}
                      <a
                        class="d:flex jc:center ai:center pos:absolute t:0 l:0 w:100% h:100% clp:rect(20px,50px,50px,20px) trf:rotateY(180deg) cur:pointer z:999"
                        href="{{ imageItem.src | image_url: width: imageItem.width }}"
                        data-pswp-width="{{ imageItem.width }}"
                        data-pswp-height="{{ imageItem.height }}"
                      >
                        <span class="w:30px h:30px bgc:color-light pos:absolute t:20px l:20px d:flex jc:center ai:center bdrs:50%">
                          <i class="fal fa-search-plus fz:18px"></i>
                        </span>
                        <img
                          class="w:100% h:100%! pos:absolute ins:0 op:0 bgc:color-light-freeze"
                          src="{{ imageItem.src | image_url: width: 10 }}"
                          alt="{{ variantItem.title }}"
                          width="{{ imageItem.width }}"
                          height="{{ imageItem.height }}"
                          data-intrinsic-width="{{ imageItem.width }}"
                          data-intrinsic-height="{{ imageItem.height }}"
                          loading="lazy"
                        >
                      </a>
                    {% endif %}
                    <div class="d:block w:100% h:100% cur:zoom-in pos:relative">
                      {% if imageItem.src != blank %}
                        <img
                          class="veda-lazyload w:100% h:100%!"
                          src="{{ imageItem.src | image_url: width: 10 }}"
                          alt="{{ variantItem.title }}"
                          width="{{ imageItem.width }}"
                          height="{{ imageItem.height }}"
                          data-intrinsic-width="{{ imageItem.width }}"
                          data-intrinsic-height="{{ imageItem.height }}"
                          loading="lazy"
                        >
                      {% else %}
                        {{ 'image' | placeholder_svg_tag: 'veda-placeholder-svg bgc:color-gray2 ' }}
                      {% endif %}
                    </div>
                  </div>
                {% endfor %}
                {% for mediaItem in product.media %}
                  {% if mediaItem.media_type == 'video' %}
                    {% liquid
                      assign arr = mediaItem.sources | where: 'format', 'mp4'
                    %}
                    <div class="swiper-slide w:100% h:auto">
                      <div
                        data-veda-video="{{ arr[0].url }}"
                      ></div>
                    </div>
                  {% endif %}
                {% endfor %}
              </div>
            </div>
            <div
              class="swiper-button-prev l:20px! w:40px h:40px d:flex! trf:translateY(-50%) bgc:color-light! bd:1px_solid_color-gray9 bdrs:50% bd:none!"
            ></div>
            <div
              class="swiper-button-next r:20px! w:40px h:40px d:flex! trf:translateY(-50%) bgc:color-light! bd:1px_solid_color-gray9 bdrs:50% bd:none!"
            ></div>
          </div>
          {% if section.settings.VEDA_switch_5 %}
            <div
              class="veda-swiper-thumbnails veda-swiper-thumbnails--vertical op:0 w:fit-content h:100% pos:absolute t:0"
              data-options="
                {
                  direction: 'vertical',
                  spaceBetween: {{ gap }},
                  {% if section.settings["VEDA_slider_6"] %}
                    slidesPerView: {{ section.settings["VEDA_slider_6"] }}
                  {% else %}
                    slidesPerView: 6
                  {% endif %}
                }
              "
            >
              <div class="swiper h:100% w:fit-content">
                <div class="swiper-wrapper w:fit-content">
                  {% for variantThumbnailsItem in product.variants %}
                    {% liquid
                      if variantThumbnailsItem.featured_image.src != blank
                        assign imageItem = variantThumbnailsItem.featured_image
                      elsif variantThumbnailsItem.image.src != blank
                        assign imageItem = variantThumbnailsItem.image
                      else
                        assign imageItem = product.images[forloop.index0]
                      endif
                    %}
                    <div class="swiper-slide cur:pointer z:1||af">
                      {% if imageItem.src != blank %}
                        <img
                          class="w:auto h:100% maw:none"
                          src="{{ imageItem.src | image_url: width: 300 }}"
                          alt="{{ variantItem.title }}"
                          width="{{ imageItem.width }}"
                          height="{{ imageItem.height }}"
                          data-intrinsic-width="{{ imageItem.width }}"
                          data-intrinsic-height="{{ imageItem.height }}"
                          loading="lazy"
                        >
                      {% else %}
                        {{ 'image' | placeholder_svg_tag: 'veda-placeholder-svg bgc:color-gray2 ' }}
                      {% endif %}
                    </div>
                  {% endfor %}
                  {% for mediaThumbnailsItem in product.media %}
                    {% if mediaThumbnailsItem.media_type == 'video' %}
                      {% liquid
                        assign arr = mediaThumbnailsItem.sources | where: 'format', 'mp4'
                      %}
                      <div class="swiper-slide cur:pointer h:auto z:1||af">
                        <div
                          data-veda-video="{{ arr[0].url }}"
                        ></div>
                      </div>
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
            </div>
          {% endif %}
        </div>
      {% endif %}
      {% comment %}KET_THUC_TACH_FILE{% endcomment %}
    </div>
  {% endif %}
{% endcapture %}

{% capture Content %}
  {% if product %}
    <div
      class="col-lg-{{ section.settings["responsive___content__lg"] }} col-md-{{ section.settings["responsive___content__md"] }} col-sm-{{ section.settings["responsive___content__sm"] }} col-xs-{{ section.settings["responsive___content__xs"] }}"
      {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
        style="{{ colGap }}"
      {% endif %}
    >
      <div class="veda-page-product-js">
        {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
        {% liquid
          assign price_compare = product.compare_at_price

          if price_compare
            assign price_compare = product.compare_at_price
          else
            assign price_compare = 0
          endif
        %}
        <script class="{{ 'product-card-data-js' }}" type="application/json">
          {
            "id": {{ product.id | json }},
            "title": {{ product.title | json }},
            "vendor": {{ product.vendor | default: "" | json }},
            "handle": {{ product.handle | default: "" | json }},
            "available": {{ product.available | default: false | json }},
            "description": {{ product.description | default: "" | json }},
            "featured_image": {{ product.featured_image | json }},
            "images": {{ product.images | default: null | json }},
            "options": {{ product.options | default: null | json }},
            "selected_or_first_available_variant": {
              "id": {{ product.selected_or_first_available_variant.id | json }},
              "available": {{ product.selected_or_first_available_variant.available | default: false | json }},
              "price": {{ product.selected_or_first_available_variant.price | money | json }},
              {% if product.selected_or_first_available_variant.compare_at_price %}
                "compare_at_price": {{ product.selected_or_first_available_variant.compare_at_price | money | json }},
              {% else %}
                "compare_at_price": {{ 0 | money | json }},
              {% endif %}
              "featured_image": {{ product.selected_or_first_available_variant.featured_image | json }},
              "image": {{ product.selected_or_first_available_variant.image | json }},
              "inventory_quantity": {{ product.selected_or_first_available_variant.inventory_quantity | json }},
              "options": {{ product.selected_or_first_available_variant.options | default: null | json }},
              "title": {{ product.selected_or_first_available_variant.title | default: "" | json }}
              {% if product.selected_or_first_available_variant.sku %}
              ,
              "sku": {{ product.selected_or_first_available_variant.sku | default: "" | json }}
              {% endif %}
              {% if product.selected_or_first_available_variant.url %}
              ,
              "url": {{ product.selected_or_first_available_variant.url | json }},
              {% endif %}
            },
            "price_max": {{ product.price_max | json }},
            "price_min": {{ product.price_min | json }},
            {% if product.type %}

            "type": {{ product.type | json }},
            {% endif %}
            "tags": {{ product.tags | default: "" | json }},
            "variants": [
            {% for item in product.variants %}
                {
                  "id": {{ item.id | json }},
                  "available": {{ item.available | default: false | json }},
                  "price": {{ item.price | money | json }},
                  {% if item.compare_at_price %}
                    "compare_at_price": {{ item.compare_at_price | money | json }},
                  {% else %}
                    "compare_at_price": {{ 0 | money | json }},
                  {% endif %}
                  "featured_image": {{ item.featured_image | json }},
                  "image": {{ item.image | json }},
                  "inventory_quantity": {{ item.inventory_quantity | json }},
                  "options": {{ item.options | default: null | json }},
                  "title": {{ item.title | json }}
                  {% if item.sku %}
                    ,
                    "sku": {{ item.sku | json }}
                  {% endif %}

                  {% if item.url %}
                  ,
                    "url": {{ item.url | json }},
                  {% endif %}
                },
            {% endfor %}
          ],
            "price": {{ product.price | money | json }},
            "compare_at_price": {{ price_compare | money | json }},
            "options_with_values": {{ product.options_with_values | json }},
            "url": {{ product.url | default: "" | json }},
            "published_at": {{ product.published_at | default: "" | json }}
          }
        </script>
        {% comment %}KET_THUC_TACH_FILE{% endcomment %}

        {% for block in section.blocks %}
          {% assign contentItem = block.settings %}

          {% if contentItem.content___enable %}
            {% capture ProductOptions %}
              {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
              {% if product.has_only_default_variant != true %}
                {% for option in product.options_with_values %}
                  <div
                    class="veda-option-title-{{ option.position }} mb:10px ff:font-secondary c:color-gray9"
                    data-css="{{ contentItem.VEDA_styleBox_8 }}"
                  >
                    {{ option.name }}: {{ option.selected_value }}
                  </div>
                  <div
                    class="veda-toggle-group-{{ option.position }} d:flex mb:10px fxw:wrap fz:14px"
                    {% if option.name != 'Color' %}
                      data-item-css="{{ contentItem.VEDA_styleBox_1 }}"
                      data-item-active-css="{{ contentItem.VEDA_styleBox_9 }}"
                    {% else %}
                      data-item-css=""
                      data-item-active-css=""
                    {% endif %}
                  >
                    {% for value in option.values %}
                      {% if option.name == 'Color' %}
                        {% liquid
                          assign bgImage = ''
                        %}
                        {% for variant in product.variants %}
                          {% liquid
                            assign i = option.position | minus: 1

                            if variant.options[i] == value and bgImage == ''
                              if variant.featured_image.src != blank
                                assign bgImage = variant.featured_image.src
                              elsif variant.image.src != blank
                                assign bgImage = variant.image.src
                              elsif product.images[i].src != blank
                                assign bgImage = product.images[i].src
                              elsif product.images[i] != blank
                                assign bgImage = product.images[i]
                              endif
                            endif
                          %}
                        {% endfor %}
                        <div
                          class="veda-toggle-group__item veda-toggle-group__item--color w:36px h:36px p:3px mr:10px mb:10px c:color-gray9 c:color-light-freeze ta:center us:none cur:pointer bd:1px_solid_color-gray3"
                          data-tooltip-position="top"
                          data-tooltip-text="{{ value }}"
                          style="border-radius: {{ contentItem.content___border_radius }}px; width: {{ contentItem.content___color_width }}px; height: {{ contentItem.content___color_height }}px"
                        >
                          <div class="w:100% h:100% pos:relative ov:hidden bdrs:inherit">
                            <div
                              class="veda-color-beauty bgz:cover bgp:center pos:absolute ins:0 z:1"
                              style="background-color: {{ value | escape }};"
                            ></div>
                            {% if bgImage %}
                              <div
                                class="veda-color-beauty bgz:cover bgp:center pos:absolute ins:0 z:2"
                                style="background-color: {{ value | escape }}; background-image: url('{{ bgImage | image_url: width: 26 }}')"
                              ></div>
                            {% else %}

                            {% endif %}
                          </div>
                        </div>
                      {% elsif option.name == 'Size' %}
                        <div
                          class="veda-toggle-group__item p:5px miw:36px mr:10px mb:10px c:color-gray9 bgc:color-gray2 ta:center us:none cur:pointer"
                          {% if option.selected_value == value %}
                            data-css="{{ contentItem.VEDA_styleBox_10 }}"
                          {% else %}
                            data-css="{{ contentItem.VEDA_styleBox_2 }}"
                          {% endif %}
                          data-tooltip-position="top"
                          data-tooltip-text="{{ value }}"
                        >
                          {{ value }}
                        </div>
                      {% else %}
                        <div
                          class="veda-toggle-group__item p:5px_15px miw:70px mr:10px mb:10px c:color-gray9 bgc:color-gray2 ta:center us:none cur:pointer"
                          {% if option.selected_value == value %}
                            data-css="{{ contentItem.VEDA_styleBox_10 }}"
                          {% else %}
                            data-css="{{ contentItem.VEDA_styleBox_2 }}"
                          {% endif %}
                          data-tooltip-position="top"
                          data-tooltip-text="{{ value }}"
                        >
                          {{ value }}
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                {% endfor %}
              {% endif %}
              {% comment %}KET_THUC_TACH_FILE{% endcomment %}
            {% endcapture %}

            {% case block.type %}
              {% when 'content_title' %}
                {% capture addon_id_0a6b827a-3348-432a-b21c-ac791bb1d959_content %}
                  {% render 'veda-addon-id_0a6b827a-3348-432a-b21c-ac791bb1d959' %}
                {% endcapture %}

                {% unless addon_id_0a6b827a-3348-432a-b21c-ac791bb1d959_content contains 'Could not find asset' %}
                  {% render 'veda-addon-id_0a6b827a-3348-432a-b21c-ac791bb1d959',
                    uniqueId: uniqueId,
                    builderMode: builderMode,
                    envatoMode: envatoMode,
                    backgroundFixed: backgroundFixed,
                    _gap: _gap,
                    imageXs: imageXs,
                    image_length: image_length,
                    imageItem: imageItem,
                    image_width: image_width,
                    arr: arr,
                    price_compare: price_compare,
                    contentItem: contentItem,
                    bgImage: bgImage,
                    i: i
                  %}
                {% endunless %}
                <h3
                  class="mb:15px fz:25px fw:500 c:color-gray9"
                  data-css="{{ contentItem.VEDA_styleBox_2 }}"
                >
                  {{ product.title }}
                </h3>
              {% when 'content_badge' %}
                <div class="d:flex">
                  {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                  {% liquid
                    assign productDate = product.published_at | date: '%s'

                    assign currentDate = 'now' | date: '%s'

                    assign difference = currentDate | minus: productDate | divided_by: 86400.0
                  %}
                  {% if true %}
                    {% if difference < 10 %}
                      <div
                        {% if 'm:0px_5px_20px_0 p:2px_10px bdrs:3px bgc:#17c478 lh:normal fz:14px ff:font-secondary lts:1px c:color-light-freeze' %}
                          class="w:fit-content {{ 'm:0px_5px_20px_0 p:2px_10px bdrs:3px bgc:#17c478 lh:normal fz:14px ff:font-secondary lts:1px c:color-light-freeze' }}"
                        {% else %}
                          class="p:5px_10px bdrs:3px bgc:#17c478 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px w:fit-content"
                        {% endif %}
                        {% if _component %}
                          component="{{ _component }}"

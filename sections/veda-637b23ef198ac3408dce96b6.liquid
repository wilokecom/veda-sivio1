{% assign uniqueId = 'id_0aafdd87-ea9b-41f2-a834-7fceecc4db0a' %}
{% assign builderMode = false %}
{% assign envatoMode = false %}
{% liquid
  if section.settings.VEDA_switch_1
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_2 == 'color'
    assign backgroundColor = 'background-color: ' | append: section.settings.VEDA_color_1 | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2
    assign bgImage = section.settings.VEDA_image_2 | append: '&width=2000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  elsif section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2 == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: section.settings.VEDA_space_4 | append: 'px;padding-right: ' | append: section.settings.VEDA_space_3 | append: 'px;padding-bottom: ' | append: section.settings.VEDA_space_1 | append: 'px;padding-left: ' | append: section.settings.VEDA_space_2 | append: 'px;'

  if section.settings.VEDA_select_1 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_1 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_1 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_1 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_2 >= 0
    assign settingGap = section.settings.VEDA_slider_2 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_2
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_3
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _prefix = false

  assign _style = false

  assign _placeholder = false

  assign _placeholder_class = false

  assign _class = false

  assign _component = false

  assign _defaul_aspect_ratio = false

  assign _image_border_radius = false
%}
{% capture Search %}
  <div class="d:flex fld:column ai:center mb:30px">
    <h3 class="ta:center fz:pfs(20px,28px) fw:500" data-css="{{ section.settings["VEDA_styleBox_4"] }}">
      {% if search.results_count == 1 %}
        {{ 'veda.templates.search.results_with_count.one' | t: count: 1, terms: search.terms | escape }}
      {% else %}
        {{
          'veda.templates.search.results_with_count.other'
          | t: count: search.results_count, terms: search.terms
          | escape
        }}
      {% endif %}
    </h3>

    <div class="w:100% maw:500px bd:1px_solid_color-gray3">
      {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
      <div
        class="veda-search-form-js bgc:color-light bdrs:6px pl:15px pr:5px trs:0.3s d:flex jc:center"
        data-text-result="{{ 'veda.templates.search.search_for' | t: terms: 'a' }}"
        data-css="{{ section.settings["VEDA_styleBox_2"] }}"
      >
        <form
          action="{{ routes.search_url }}"
          method="GET"
          class="w:100% d:flex ai:center jc:space-between"
        >
          <div class="d:flex ai:center w:100%">
            <input
              class="rs w:100% h:44px bd:0! bxsh:none! o:none! bgc:transparent ff:font-secondary fz:16px c:color-gray8 c:color-gray6||ph trs:0.3s"
              data-css="{{ section.settings["VEDA_styleBox_6"] }}"
              type="text"
              placeholder="{{ 'veda.general.search.search' | t }}"
              name="q"
              value="{{ search.terms | escape }}"
              autocorrect="off"
              autocomplete="off"
            >
            <div
              class="veda-spinner bdw:2px mr:10px d:none"
              style="--spinner-color: var(--color-primary); --spinner-size: 14px;display: none"
            ></div>
            {% if section.settings.veda_search___type %}
              <input type="hidden" name="type" value="{{ section.settings["veda_search___type"] | replace: ' ', '' }}">
            {% else %}
              <input type="hidden" name="type" value="product,article,collection,page">
            {% endif %}
            {% if section.settings.VEDA_select_4 %}
              <input type="hidden" name="options[unavailable_products]" value="last">
            {% else %}
              <input type="hidden" name="options[unavailable_products]" value="{{ section.settings["VEDA_select_4"] }}">
            {% endif %}
            {% if _prefix %}
              <input type="hidden" name="options[prefix]" value="{{ _prefix }}">
            {% else %}
              <input type="hidden" name="options[prefix]" value="last">
            {% endif %}
            {% if section.settings.veda_search___fields %}
              <input type="hidden" name="options[fields]" value="{{ section.settings["veda_search___fields"] }}">
            {% else %}
              <input type="hidden" name="options[fields]" value="title,vendor,product_type,variants.title">
            {% endif %}
            {% if section.settings.VEDA_slider_6 %}
              <input type="hidden" name="limit" value="{{ section.settings["VEDA_slider_6"] }}">
            {% else %}
              <input type="hidden" name="limit" value="10">
            {% endif %}
          </div>
          <button
            class="rs flxs:0 bxsh:none bd:0 bgc:transparent! bxsh:none! o:none! fz:22px c:color-gray8 c:color-primary|h trs:0.3s cur:pointer"
            data-css="{{ section.settings["VEDA_styleBox_8"] }}"
            type="submit"
            aria-label="Search"
          >
            <i class="far fa-search"></i>
          </button>
        </form>
      </div>
      {% comment %}KET_THUC_TACH_FILE{% endcomment %}
    </div>
  </div>
{% endcapture %}

{% capture Content %}
  {% liquid
    if section.settings.VEDA_slider_10
      assign imageRadius = 'border-radius:' | append: section.settings.VEDA_slider_10 | append: 'px;'
    endif

    assign aspectRatio = 0.75

    if section.settings.VEDA_slider_9
      assign aspectRatio = 100 | divided_by: section.settings.VEDA_slider_9
    endif

    assign quantity_displayed = 8

    if section.settings.VEDA_slider_11
      assign quantity_displayed = section.settings.VEDA_slider_11
    endif
  %}

  <div
    class="row row-cols-lg-{{ section.settings["column__lg"] }} row-cols-md-{{ section.settings["column__md"] }} row-cols-sm-{{ section.settings["column__sm"] }} row-cols-{{ section.settings["column__xs"] }}"
    {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
      style="{{ rowGap }}"
    {% endif %}
  >
    {% paginate search.results by quantity_displayed %}
      {% for searchItem in search.results %}
        {% liquid
          assign articleCond = false

          assign pageCond = false

          assign productCond = false

          if searchItem.object_type == 'article'
            assign articleCond = true
          elsif searchItem.object_type == 'page'
            assign pageCond = true
          elsif searchItem.object_type == 'product'
            assign productCond = true
          endif
        %}
        {% if articleCond %}
          <div
            class="col"
            {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
              style="{{ colGap }}"
            {% endif %}
          >
            {% liquid
              assign articleItem = searchItem
            %}
            <div class="d:flex fld:column">
              <a
                href="{{ articleItem.url }}"
                class="veda-image-cover d:block"
                style="--aspect-ratio: {{ aspectRatio }};border-radius: {{ imageRadius }}px"
              >
                {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                {% if articleItem.image.src != blank %}
                  <img
                    class="veda-lazyload {{ _class }}"
                    src="{{ articleItem.image.src | image_url: width: 10 }}"
                    draggable="false"
                    {% if articleItem.title %}
                      alt="{{ articleItem.title }}"
                    {% else %}
                      alt="image"
                    {% endif %}
                    {% if articleItem.image.width %}
                      data-intrinsic-width="{{ articleItem.image.width }}"
                      width="{{ articleItem.image.width }}"
                    {% endif %}
                    {% if articleItem.image.height %}
                      height="{{ articleItem.image.height }}"
                      data-intrinsic-height="{{ articleItem.image.height }}"
                    {% endif %}
                    {% if _style %}
                      style="{{ _style }}"
                    {% endif %}
                    loading="lazy"
                  >
                {% else %}
                  {% liquid
                    assign pl = 'lifestyle-1'

                    if _placeholder
                      assign pl = _placeholder
                    endif

                    assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _class

                    if _placeholder_class
                      assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
                    endif
                  %}
                  {{ pl | placeholder_svg_tag: placeholder_class }}
                {% endif %}
                {% comment %}KET_THUC_TACH_FILE{% endcomment %}
              </a>

              <div class="pt:15px">
                {% liquid
                  assign tagSize = articleItem.tags | size
                %}
                {% if tagSize >= 1 and section.settings.VEDA_switch_6 %}
                  <div
                    class="mb:5px c:color-gray9 fz:11px fw:500 ff:font-secondary lts:1px tt:uppercase"
                    data-css="{{ section.settings["VEDA_styleBox_16"] }}"
                  >
                    {{ articleItem.tags[0] | link_to_tag: articleItem.tags[0] }}
                  </div>
                {% endif %}
                <a
                  href="{{ articleItem.url }}"
                  class="lines-3 c:color-gray9 fz:18px fw:500 ff:font-secondary cur:pointer trs:0.3s"
                  data-css="{{ section.settings["VEDA_styleBox_10"] }}"
                >
                  {{ articleItem.title }}
                </a>
                <div class="d:flex ai:center c:color-gray7 fz:13px">
                  {% if section.settings.VEDA_switch_4 and articleItem.published_at %}
                    <div class="mt:5px" data-css="{{ section.settings["VEDA_styleBox_12"] }}">
                      <i class="far fa-calendar-alt"></i>
                      <span class="ff:font-secondary ml:3px">{{ articleItem.published_at | date: '%b %d, %Y' }}</span>
                    </div>
                  {% endif %}
                  {% if section.settings.VEDA_switch_4
                    and articleItem.published_at
                    and section.settings.VEDA_switch_5
                    and articleItem.comments_enabled
                  %}
                    <div class="ml:10px mr:10px c:color-gray5">.</div>
                  {% endif %}
                  {% if section.settings.VEDA_switch_5 and articleItem.comments_enabled %}
                    <div class="mt:5px" data-css="{{ section.settings["VEDA_styleBox_14"] }}">
                      {% if articleItem.comments_count > 1 %}
                        <i class="far fa-comments"></i>
                        <span class="ff:font-secondary ml:3px tt:capitalize">
                          {{ 'veda.blogs.article.comments.other' | t: count: articleItem.comments_count }}
                        </span>
                      {% else %}
                        <i class="far fa-comment"></i>
                        <span class="ff:font-secondary ml:3px tt:capitalize">
                          {{ 'veda.blogs.article.comments.one' | t: count: articleItem.comments_count }}
                        </span>
                      {% endif %}
                    </div>
                  {% endif %}
                </div>
                {% if section.settings.VEDA_switch_7 %}
                  <div class="mt:15px lines-3" data-css="{{ section.settings["VEDA_styleBox_18"] }}">
                    {% liquid
                      assign excerptSize = articleItem.excerpt | size
                    %}
                    {% if excerptSize > 0 %}
                      {{ articleItem.excerpt | strip_html | truncatewords: 24, '...' }}
                    {% elsif articleItem.article_settings %}
                      {{ articleItem.content | strip_html | truncatewords: 24, '...' }}
                    {% endif %}
                  </div>
                {% endif %}
              </div>
            </div>
          </div>
        {% elsif pageCond %}
          <div
            class="col"
            {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
              style="{{ colGap }}"
            {% endif %}
          >
            <a
              href="{{ searchItem.url }}"
              class="d:flex fld:column jc:center mih:250px p:20px bgc:color-gray2 bdrs:15px"
              data-css="{{ section.settings["VEDA_styleBox_28"] }}"
            >
              <h3
                class="lines-3 c:color-gray9 fz:18px fw:500 cur:pointer trs:0.3s"
                data-css="{{ section.settings["VEDA_styleBox_20"] }}"
              >
                {{ searchItem.title }}
              </h3>
              <div class="d:flex ai:center c:color-gray7 fz:13px">
                {% if searchItem.published_at %}
                  <div class="mt:5px" data-css="{{ section.settings["VEDA_styleBox_22"] }}">
                    <i class="far fa-calendar-alt"></i>
                    <span class="ff:font-secondary ml:3px">{{ searchItem.published_at | date: '%b %d, %Y' }}</span>
                  </div>
                {% endif %}
                {% if searchItem.published_at and searchItem.author %}
                  <div class="ml:10px mr:10px c:color-gray5">.</div>
                {% endif %}
                {% if searchItem.author %}
                  <div class="mt:5px" data-css="{{ section.settings["VEDA_styleBox_24"] }}">
                    <i class="far fa-user"></i>
                    <span class="ff:font-secondary ml:3px">
                      {{ searchItem.author }}
                    </span>
                  </div>
                {% endif %}
              </div>
              {% if searchItem.content %}
                <div class="mt:15px lines-3" data-css="{{ section.settings["VEDA_styleBox_26"] }}">
                  {{ searchItem.content | strip_html | truncatewords: 24, '...' }}
                </div>
              {% endif %}
            </a>
          </div>
        {% elsif productCond %}
          {% liquid
            assign productItem = searchItem
          %}
          <div
            class="col"
            {% if section.settings.VEDA_slider_2 or section.settings.VEDA_slider_2 == 0 %}
              style="{{ colGap }}"
            {% endif %}
          >
            <div
              class="product-card-js group* pos:relative mih:100%"
              data-css="{{ section.settings["VEDA_styleBox_30"] }}"
            >
              {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
              {% liquid
                assign price_compare = productItem.compare_at_price

                if price_compare
                  assign price_compare = productItem.compare_at_price
                else
                  assign price_compare = 0
                endif
              %}
              <script class="{{ 'product-card-data-js' }}" type="application/json">
                {
                  "id": {{ productItem.id | json }},
                  "title": {{ productItem.title | json }},
                  "vendor": {{ productItem.vendor | default: "" | json }},
                  "handle": {{ productItem.handle | default: "" | json }},
                  "available": {{ productItem.available | default: false | json }},
                  "description": {{ productItem.description | default: "" | json }},
                  "featured_image": {{ productItem.featured_image | json }},
                  "images": {{ productItem.images | default: null | json }},
                  "options": {{ productItem.options | default: null | json }},
                  "selected_or_first_available_variant": {
                    "id": {{ productItem.selected_or_first_available_variant.id | json }},
                    "available": {{ productItem.selected_or_first_available_variant.available | default: false | json }},
                    "price": {{ productItem.selected_or_first_available_variant.price | money | json }},
                    {% if productItem.selected_or_first_available_variant.compare_at_price %}
                      "compare_at_price": {{ productItem.selected_or_first_available_variant.compare_at_price | money | json }},
                    {% else %}
                      "compare_at_price": {{ 0 | money | json }},
                    {% endif %}
                    "featured_image": {{ productItem.selected_or_first_available_variant.featured_image | json }},
                    "image": {{ productItem.selected_or_first_available_variant.image | json }},
                    "inventory_quantity": {{ productItem.selected_or_first_available_variant.inventory_quantity | json }},
                    "options": {{ productItem.selected_or_first_available_variant.options | default: null | json }},
                    "title": {{ productItem.selected_or_first_available_variant.title | default: "" | json }}
                    {% if productItem.selected_or_first_available_variant.sku %}
                    ,
                    "sku": {{ productItem.selected_or_first_available_variant.sku | default: "" | json }}
                    {% endif %}
                    {% if productItem.selected_or_first_available_variant.url %}
                    ,
                    "url": {{ productItem.selected_or_first_available_variant.url | json }},
                    {% endif %}
                  },
                  "price_max": {{ productItem.price_max | json }},
                  "price_min": {{ productItem.price_min | json }},
                  {% if productItem.type %}

                  "type": {{ productItem.type | json }},
                  {% endif %}
                  "tags": {{ productItem.tags | default: "" | json }},
                  "variants": [
                  {% for item in productItem.variants %}
                      {
                        "id": {{ item.id | json }},
                        "available": {{ item.available | default: false | json }},
                        "price": {{ item.price | money | json }},
                        {% if item.compare_at_price %}
                          "compare_at_price": {{ item.compare_at_price | money | json }},
                        {% else %}
                          "compare_at_price": {{ 0 | money | json }},
                        {% endif %}
                        "featured_image": {{ item.featured_image | json }},
                        "image": {{ item.image | json }},
                        "inventory_quantity": {{ item.inventory_quantity | json }},
                        "options": {{ item.options | default: null | json }},
                        "title": {{ item.title | json }}
                        {% if item.sku %}
                          ,
                          "sku": {{ item.sku | json }}
                        {% endif %}

                        {% if item.url %}
                        ,
                          "url": {{ item.url | json }},
                        {% endif %}
                      },
                  {% endfor %}
                ],
                  "price": {{ productItem.price | money | json }},
                  "compare_at_price": {{ price_compare | money | json }},
                  "options_with_values": {{ productItem.options_with_values | json }},
                  "url": {{ productItem.url | default: "" | json }},
                  "published_at": {{ productItem.published_at | default: "" | json }}
                }
              </script>
              {% comment %}KET_THUC_TACH_FILE{% endcomment %}

              <script type="application/json" class="quickview-data-js">
                {
                  add_to_cart_text: "{{ 'veda.products.product.add_to_cart' | t }}",
                  quantity_text: "{{ 'veda.products.product.quantity.label' | t }}",
                  sold_out_text: "{{ 'veda.products.product.sold_out' | t }}",
                  view_detail: "{{ 'veda.products.product.view_details' | t }}",
                  add_compare: "{{ 'veda.products.product.add_to_compare' | t }}",
                  remove_compare: "{{ 'veda.products.product.remove_from_compare' | t }}",
                  add_wish_list: "{{ 'veda.products.product.add_to_wishlist' | t }}",
                  remove_wish_list: "{{ 'veda.products.product.remove_from_wishlist' | t }}"
                }
              </script>
              <script type="application/json" class="current-variant-id">
                {{ productItem.selected_or_first_available_variant.id | json }}
              </script>
              <div class="pos:absolute t:10px l:10px z:12 d:flex fld:column">
                {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                {% liquid
                  assign productDate = productItem.published_at | date: '%s'

                  assign currentDate = 'now' | date: '%s'

                  assign difference = currentDate | minus: productDate | divided_by: 86400.0
                %}
                {% if section.settings.new___enable %}
                  {% if difference < 10 %}
                    <div
                      {% if 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' %}
                        class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#17c478 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px' }}"
                      {% else %}
                        class="p:5px_10px bdrs:3px bgc:#17c478 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze mb:5px w:fit-content"
                      {% endif %}
                      {% if 'new' %}
                        component="{{ 'new' }}"
                      {% endif %}
                      data-css="{{ section.settings["new___design__id"] }}"
                    >
                      {{ 'veda.products.product.new' | t }}
                    </div>
                  {% endif %}
                {% endif %}
                {% comment %}KET_THUC_TACH_FILE{% endcomment %}
                {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                {% if productItem.compare_at_price > productItem.price and section.settings.sale___enable %}
                  <div
                    {% if 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' %}
                      class="w:fit-content {{ 'p:3px_10px bdrs:3px bgc:#eb4747 lh:normal fz:12px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze' }}"
                    {% else %}
                      class="p:5px_10px bdrs:3px bgc:#eb4747 lh:normal fz:10px ff:font-secondary tt:uppercase lts:1px c:color-light-freeze w:fit-content"
                    {% endif %}
                    {% if 'sale' %}
                      component="{{ 'sale' }}"
                    {% endif %}
                    data-css="{{ section.settings["sale___design__id"] }}"
                  >
                    {% liquid
                      assign ratio = productItem.price | times: 100.0 | divided_by: productItem.compare_at_price
                    %}
                    -{{ 100 | minus: ratio | ceil }}%
                  </div>
                {% endif %}
                {% comment %}KET_THUC_TACH_FILE{% endcomment %}
              </div>
              <div class="w:100% pos:relative">
                {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                {% liquid
                  if _defaul_aspect_ratio and _defaul_aspect_ratio != 0
                    assign aspectRatio = _defaul_aspect_ratio
                  else
                    assign aspectRatio = 90
                  endif

                  if section.settings.VEDA_slider_9 and section.settings.VEDA_slider_9 != 0
                    assign aspectRatio = section.settings.VEDA_slider_9
                  endif
                %}
                <div
                  class="ov:hidden w:100% pos:relative"
                  {% if section.settings.VEDA_slider_10 %}
                    style="border-radius: {{ section.settings["VEDA_slider_10"] }}px"
                  {% endif %}
                >
                  <a
                    href="{{ productItem.url }}"
                    class="veda-image-cover pos:relative d:block trs:opacity_0.3s z:1 {% if productItem.images[1] %}op:0*group-hover{% endif %}"
                    style="--aspect-ratio:{{ aspectRatio }}"
                  >
                    <img
                      class="product-card-image-js veda-lazyload"
                      src="{{ productItem.featured_image.src | image_url: width: 10 }}"
                      alt="{{ productItem.title }}"
                      width="{{ productItem.featured_image.width }}"
                      height="{{ productItem.featured_image.height }}"
                      data-intrinsic-width="{{ productItem.featured_image.width }}"
                      data-intrinsic-height="{{ productItem.featured_image.height }}"
                      loading="lazy"
                    >
                  </a>
                  {% if productItem.images[1] %}
                    <a
                      href="{{ productItem.url }}"

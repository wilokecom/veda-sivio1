                  aria-label="Previous"
                  href="{{ paginate.previous.url }}"
                  data-css="{{ section.settings["VEDA_styleBox_47"] }}"
                >
                  <i class="far fa-arrow-left"></i>
                </a>
              </li>
            {% endif %}
            {% for part in paginate.parts %}
              {% if part.is_link %}
                <li>
                  <a
                    class="{{ item_general_class }} {{ item_variant_class }}"
                    aria-label="Page {{ part.title }}"
                    href="{{ part.url }}"
                    data-css="{{ section.settings["VEDA_styleBox_47"] }}"
                  >
                    {{ part.title }}
                  </a>
                </li>
              {% else %}
                {% if part.title == paginate.current_page %}
                  <li>
                    <span
                      class="{{ item_general_class }} {{ item_variant_class }} bgc:color-primary! c:color-light-freeze!"
                      aria-current="page"
                      aria-label="Page {{ part.title }}"
                      data-css="{{ section.settings["VEDA_styleBox_49"] }}"
                    >
                      {{ part.title }}
                    </span>
                  </li>
                {% else %}
                  <li>
                    <span
                      class="{{ item_general_class }} {{ item_variant_class }}"
                      aria-label="Page {{ part.title }}"
                      data-css="{{ section.settings["VEDA_styleBox_47"] }}"
                    >
                      {{ part.title }}
                    </span>
                  </li>
                {% endif %}
              {% endif %}
            {% endfor %}

            {% if paginate.next %}
              <li>
                <a
                  class="{{ item_general_class }} {{ item_variant_class }}"
                  aria-label="Next"
                  href="{{ paginate.next.url }}"
                  data-css="{{ section.settings["VEDA_styleBox_47"] }}"
                >
                  <i class="far fa-arrow-right"></i>
                </a>
              </li>
            {% endif %}
          </ol>
        </nav>
        {% comment %}KET_THUC_TACH_FILE{% endcomment %}
      {% endif %}
    {% endpaginate %}
  </div>

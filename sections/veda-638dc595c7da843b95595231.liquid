          "name": "{{ section.settings["VEDA_flexOrder_2"] }}",
          "price": "{{ section.settings["VEDA_flexOrder_3"] }}",
          "color": "{{ section.settings["VEDA_flexOrder_4"] }}"
        },
        "aspect_ratio": "{{ section.settings["VEDA_slider_6"] | json }}",
        "image_border_radius": "{{ section.settings["VEDA_slider_7"] | json }}"
      },
      "sale": {
        "enable": "{{ section.settings["sale___enable"] | json }}",
        "design": {
          "css": "{{ section.settings["sale___design__css"] }}"
        }
      },
      "new": {
        "enable": "{{ section.settings["new___enable"] | json }}",
        "design": {
          "css": "{{ section.settings["new___design__css"] }}"
        }
      },
      "sold_out": {
        "enable": "{{ section.settings["sold_out___enable"] | json }}",
        "design": {
          "css": "{{ section.settings["sold_out___design__css"] }}"
        }
      },
      "wish_list_action": {
        "enable": "{{ section.settings["VEDA_switch_6"] | json }}",
        "design": {
          "css": "{{ section.settings["VEDA_styleBox_19"] }}"
        }
      },
      "compare_action": {
        "enable": "{{ section.settings["compare_action___enable"] | json }}",
        "design": {
          "css": "{{ section.settings["VEDA_styleBox_17"] }}"
        }
      },
      "quick_view": {
        "enable": "{{ section.settings["quick_view___enable"] | json }}",

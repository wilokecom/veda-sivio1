{% assign uniqueId = 'id_f8d4eb4f-f01b-42af-a258-531eb8b64ed6' %}
{% assign builderMode = false %}
{% assign envatoMode = false %}
{% liquid
  if section.settings.VEDA_switch_1
    assign backgroundFixed = 'fixed'
  else
    assign backgroundFixed = 'scroll'
  endif

  if section.settings.VEDA_select_2 == 'color'
    assign backgroundColor = 'background-color: ' | append: section.settings.VEDA_color_1 | append: ';'
  else
    assign backgroundColor = ''
  endif

  if section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2
    assign bgImage = section.settings.VEDA_image_2 | append: '&width=2000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  elsif section.settings.VEDA_select_2 == 'image' and section.settings.VEDA_image_2 == blank
    assign bgImage = 'https://cdn.shopify.com/s/files/1/0552/5510/5616/files/udxhtnzxtq.png?v=1681903122&width=1000'

    assign backgroundImage = 'background-image: url(' | append: bgImage | append: ');background-attachment: ' | append: backgroundFixed | append: ';background-size: ' | append: section.settings.VEDA_select_3 | append: ';'
  else
    assign backgroundImage = ''
  endif

  assign sectionPadding = 'padding-top: ' | append: section.settings.VEDA_space_4 | append: 'px;padding-right: ' | append: section.settings.VEDA_space_3 | append: 'px;padding-bottom: ' | append: section.settings.VEDA_space_1 | append: 'px;padding-left: ' | append: section.settings.VEDA_space_2 | append: 'px;'

  if section.settings.VEDA_select_1 == 'full_width'
    assign containerType = 'container-fluid'
  else
    assign containerType = 'container'
  endif

  if section.settings.VEDA_slider_1 >= 0
    assign containerGap = 'padding-left: ' | append: section.settings.VEDA_slider_1 | append: 'px; padding-right: ' | append: section.settings.VEDA_slider_1 | append: 'px;'
  else
    assign containerGap = ''
  endif

  if section.settings.section_settings___width >= 0
    assign containerWidth = 'max-width: ' | append: section.settings.section_settings___width | append: 'px;'
  else
    assign containerWidth = ''
  endif

  if section.settings.VEDA_slider_2 >= 0
    assign settingGap = section.settings.VEDA_slider_2 | divided_by: 2.0

    assign settingGapVertical = section.settings.VEDA_slider_2
  else
    assign settingGap = ''

    assign settingGapVertical = ''
  endif

  assign colGap = 'margin-top: ' | append: settingGapVertical | append: 'px; padding-left: ' | append: settingGap | append: 'px; padding-right: ' | append: settingGap | append: 'px;'

  assign rowGap = 'margin-top: -' | append: settingGapVertical | append: 'px; margin-left: -' | append: settingGap | append: 'px; margin-right: -' | append: settingGap | append: 'px;'

  assign sectionSeparator = ''

  if section.settings.VEDA_switch_3
    if section.settings.VEDA_slider_4
      assign sectionSeparator = sectionSeparator | append: 'border-top: ' | append: section.settings.VEDA_slider_4 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif

    if section.settings.VEDA_slider_5
      assign sectionSeparator = sectionSeparator | append: 'border-bottom: ' | append: section.settings.VEDA_slider_5 | append: 'px solid ' | append: section.settings.VEDA_color_3 | append: ';'
    endif
  endif

  if colGap and rowGap and builderMode and envatoMode
  endif
%}

{% liquid
  assign _alt = false

  assign _style = false

  assign _placeholder = false

  assign _placeholder_class = false

  assign _class = false

  assign _component = false
%}
{% capture SliderPager %}
  {% liquid
    assign generalClassName = 'group* pos:absolute t:50% z:9 trf:translateY(-50%) cur:pointer bgc:color-light bgc:color-primary|h c:color-gray9 c:color-light-freeze|h fz:22px w:pfs(30px,40px) trs:0.4s h:pfs(60px,80px) d:flex ai:center jc:center ov:hidden d:none@+md'
  %}

  {% if section.settings.VEDA_switch_9 %}
    <div class="veda-animated-slider__arrow">
      <div
        class="veda-animated-slider__arrow-left l:0 {{ generalClassName }}"
        data-css="{{ section.settings["VEDA_styleBox_22"] }}"
      >
        <i class="far fa-angle-left"></i>
      </div>
      <div
        class="veda-animated-slider__arrow-right r:0 {{ generalClassName }}"
        data-css="{{ section.settings["VEDA_styleBox_22"] }}"
      >
        <i class="far fa-angle-right"></i>
      </div>
    </div>
  {% endif %}

  {% if section.settings.VEDA_switch_10 %}
    <div
      class="veda-animated-slider__pagination pos:absolute z:9 l:50% b:20px trf:translateX(-50%)"
      data-animated-pagination-active-class="bgc:color-primary! trf:scale(2)"
    >
      {% for block in section.blocks %}
        {% if block.type == 'content' %}
          {% assign paginationItem = block.settings %}

          <div
            class="veda-animated-slider__pagination-item w:5px h:5px bgc:color-light m:5px trs:0.3s"
            data-css="{{ section.settings["VEDA_styleBox_24"] }}"
          ></div>
        {% endif %}
      {% endfor %}
    </div>
  {% endif %}
{% endcapture %}

{% capture Content %}
  {% liquid
    assign delay = 4000

    if section.settings.slider_settings___delay
      assign delay = section.settings.slider_settings___delay
    endif
  %}
  <div
    class="veda-animated-slider"
    data-options="
      {
        autoPlay: {{ section.settings["VEDA_switch_8"] }},
        delay: {{ delay }},
        loop: {{ section.settings["slider_settings___loop"] }}
      }
    "
  >
    <div
      class="veda-animated-slider__inner mih:260px mih:400px@md mih:var(--height-slider)!@lg mah:100vh@lg"
      style="--height-slider: {{ section.settings["height"] }}px"
    >
      {% for block in section.blocks %}
        {% if block.type == 'content' %}
          {% assign item = block.settings %}

          <div class="veda-animated-slider__slide d:flex fld:column bgz:cover bgp:center ov:hidden">
            {% if item.content___media_type == 'image' %}
              <div
                data-animated-in="{{ item.VEDA_animateInOut_9 }}"
                data-animated-out="{{ item.VEDA_animateInOut_10 }}"
                class="veda-image-cover p:0! pos:absolute ins:0 z:-2"
              >
                {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                {% if item.content___image__src != blank %}
                  <img
                    class="veda-lazyload {{ _class }}"
                    src="{{ item.content___image__src | append: '&width=10' }}"
                    draggable="false"
                    {% if _alt %}
                      alt="{{ _alt }}"
                    {% else %}
                      alt="image"
                    {% endif %}
                    {% if item.content___image__width %}
                      data-intrinsic-width="{{ item.content___image__width }}"
                      width="{{ item.content___image__width }}"
                    {% endif %}
                    {% if item.content___image__height %}
                      height="{{ item.content___image__height }}"
                      data-intrinsic-height="{{ item.content___image__height }}"
                    {% endif %}
                    {% if _style %}
                      style="{{ _style }}"
                    {% endif %}
                  >
                {% else %}
                  {% liquid
                    assign pl = 'lifestyle-1'

                    if _placeholder
                      assign pl = _placeholder
                    endif

                    assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _class

                    if _placeholder_class
                      assign placeholder_class = 'veda-placeholder-svg bgc:color-gray2 ' | append: _placeholder_class
                    endif
                  %}
                  {{ pl | placeholder_svg_tag: placeholder_class }}
                {% endif %}
                {% comment %}KET_THUC_TACH_FILE{% endcomment %}
              </div>
            {% endif %}
            {% if item.content___video and item.content___media_type == 'video' %}
              <div
                data-animated-in="{{ item.VEDA_animateInOut_9 }}"
                data-animated-out="{{ item.VEDA_animateInOut_10 }}"
                class="pos:absolute ins:0 z:-2"
              >
                <div data-veda-video="{{ item.content___video }}"></div>
              </div>
            {% endif %}
            <div
              class="d:flex ai:center h:100% {{ containerType }}"
              {% if section.settings.VEDA_slider_1
                or section.settings.VEDA_slider_1 == 0
                or section.settings.section_settings___width
                or section.settings.section_settings___width == 0
              %}
                style="{{ containerGap }}{{ containerWidth }}"
              {% endif %}
            >
              <div
                class="pos:relative d:flex w:100% pt:30px pb:50px"
                style="justify-content: {{ item.content___position }};"
              >
                <div
                  class="w:40% w:60%@+md!"
                  data-css="{{ item.VEDA_styleBox_2 }}"
                >
                  <div>
                    {% if item.content___sub_title %}
                      <h4
                        data-animated-in-class="animdel:0.2s"
                        data-animated-out-class="animdel:0.2s"
                        data-animated-in="{{ item.VEDA_animateInOut_1 }}"
                        data-animated-out="{{ item.VEDA_animateInOut_2 }}"
                        class="ff:font-secondary fz:pfs(16px,22px) fw:500 mb:5px c:color-gray9-freeze"
                        data-css="{{ item.VEDA_styleBox_4 }}"
                      >
                        {{ item.content___sub_title }}
                      </h4>
                    {% endif %}
                    {% if item.content___title %}
                      <h2
                        data-animated-in-class="animdel:0.4s"
                        data-animated-out-class="animdel:0.4s"
                        data-animated-in="{{ item.VEDA_animateInOut_3 }}"
                        data-animated-out="{{ item.VEDA_animateInOut_4 }}"
                        class="ff:font-secondary fz:pfs(30px,44px) fw:600 mb:pfs(10px,20px) lh:1.3 c:color-gray9-freeze"
                        data-css="{{ item.VEDA_styleBox_6 }}"
                      >
                        {{ item.content___title }}
                      </h2>
                    {% endif %}
                    {% if item.content___description %}
                      <div
                        data-animated-in-class="animdel:0.6s"
                        data-animated-out-class="animdel:0.6s"
                        data-animated-in="{{ item.VEDA_animateInOut_5 }}"
                        data-animated-out="{{ item.VEDA_animateInOut_6 }}"
                        data-css="{{ item.VEDA_styleBox_8 }}"
                        class="fz:pfs(14px,18px) d:none@+md c:color-gray7-freeze"
                      >
                        {{ item.content___description }}
                      </div>
                    {% endif %}
                  </div>

                  <div
                    data-animated-in-class="animdel:0.8s"
                    data-animated-out-class="animdel:0.8s"
                    data-animated-in="{{ item.VEDA_animateInOut_7 }}"
                    data-animated-out="{{ item.VEDA_animateInOut_8 }}"
                    class="d:inline-flex fxw:wrap"
                  >
                    {% if item.VEDA_switch_4 %}
                      <div>
                        {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                        {% liquid
                          if item.VEDA_select_4 == 'link'
                            assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s '
                          else
                            assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s p:13px_20px bd:none bxsh:none o:none cur:pointer '
                          endif

                          assign default_for_bottom_class = ''

                          case item.VEDA_select_4
                            when 'primary'
                              assign variant_class = ' bgc:color-primary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                            when 'secondary'
                              assign variant_class = ' bgc:color-secondary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                            when 'dark'
                              assign variant_class = ' bgc:color-gray9 c:color-light bgc:color-primary|h c:color-light-freeze|h'
                            when 'gray'
                              assign variant_class = ' bgc:color-gray3 c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                            when 'light'
                              assign variant_class = ' bgc:color-light c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                            else
                              assign variant_class = ' c:color-gray9 c:color-primary|h'
                          endcase
                        %}

                        <a
                          component="{{ _component }}"
                          {% if 'bdrs:6px mt:10px mr:10px p:8px_15px@+md' %}
                            class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }} {{ 'bdrs:6px mt:10px mr:10px p:8px_15px@+md' }}"
                          {% else %}
                            class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }}"
                          {% endif %}
                          {% if item.content___new_tab_left %}
                            target="_blank"
                          {% endif %}
                          data-css="{{ item.VEDA_styleBox_10 }}"
                          href="{{ item.VEDA_linkPicker_1 }}"
                        >
                          {% if item.VEDA_switch_5 and item.VEDA_radioGroup_1 == 'left' %}
                            <span
                              class="mr:10px"
                              data-css="{{ item.VEDA_styleBox_14 }}"
                            >
                              {{ item.VEDA_icon_1 }}
                            </span>
                          {% endif %}
                          <span>
                            {{ item.VEDA_text_1 }}
                          </span>
                          {% if item.VEDA_switch_5 and item.VEDA_radioGroup_1 == 'right' %}
                            <span
                              class="ml:10px"
                              data-css="{{ item.VEDA_styleBox_14 }}"
                            >
                              {{ item.VEDA_icon_1 }}
                            </span>
                          {% endif %}
                        </a>
                        {% comment %}KET_THUC_TACH_FILE{% endcomment %}
                      </div>
                    {% endif %}

                    {% if item.VEDA_switch_6 %}
                      <div>
                        {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
                        {% liquid
                          if item.VEDA_select_5 == 'link'
                            assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s '
                          else
                            assign default_class = 'rs d:inline-block td:none lh:1.5 fz:15px ff:font-primary fw:500 trs:0.3s p:13px_20px bd:none bxsh:none o:none cur:pointer '
                          endif

                          assign default_for_bottom_class = ''

                          case item.VEDA_select_5
                            when 'primary'
                              assign variant_class = ' bgc:color-primary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                            when 'secondary'
                              assign variant_class = ' bgc:color-secondary c:color-light-freeze bgc:color-gray9|h c:color-light|h'
                            when 'dark'
                              assign variant_class = ' bgc:color-gray9 c:color-light bgc:color-primary|h c:color-light-freeze|h'
                            when 'gray'
                              assign variant_class = ' bgc:color-gray3 c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                            when 'light'
                              assign variant_class = ' bgc:color-light c:color-gray9 bgc:color-primary|h c:color-light-freeze|h'
                            else
                              assign variant_class = ' c:color-gray9 c:color-primary|h'
                          endcase
                        %}

                        <a
                          component="{{ _component }}"
                          {% if 'bdrs:6px mt:10px' %}
                            class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }} {{ 'bdrs:6px mt:10px' }}"
                          {% else %}
                            class="{{ default_class }}{{ default_for_bottom_class }}{{ variant_class }}"
                          {% endif %}
                          {% if item.content___new_tab_right %}
                            target="_blank"
                          {% endif %}
                          data-css="{{ item.VEDA_styleBox_16 }}"
                          href="{{ item.VEDA_linkPicker_2 }}"
                        >
                          {% if item.VEDA_switch_7 and item.VEDA_radioGroup_2 == 'left' %}
                            <span
                              class="mr:10px"
                              data-css="{{ item.VEDA_styleBox_20 }}"
                            >
                              {{ item.VEDA_icon_2 }}
                            </span>
                          {% endif %}
                          <span>
                            {{ item.VEDA_text_2 }}
                          </span>
                          {% if item.VEDA_switch_7 and item.VEDA_radioGroup_2 == 'right' %}
                            <span
                              class="ml:10px"
                              data-css="{{ item.VEDA_styleBox_20 }}"
                            >
                              {{ item.VEDA_icon_2 }}
                            </span>
                          {% endif %}
                        </a>
                        {% comment %}KET_THUC_TACH_FILE{% endcomment %}
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
          </div>
        {% endif %}
      {% endfor %}
    </div>
    {{ SliderPager }}
  </div>
{% endcapture %}

<section
  data-id="{{ uniqueId }}"
  class="veda-lazyload pos:relative z:1 bgz:cover bgp:center"
  style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
>
  {{ Content }}
  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</section>

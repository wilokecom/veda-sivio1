{% capture Actions %}
  <div class="h:100% d:flex jc:flex-end ai:center fz:18px c:color-gray9">
    {% for block in section.blocks %}
      {% assign action = block.settings %}

      {% if action.right_actions___enable %}
        {% if block.type == 'right_actions_wishlist' %}
          <a
            href="{{ action.right_actions___link }}"
            {% if action.right_actions___new_tab %}
              target="_blank"
            {% endif %}
            class="veda-wishlist-badge-toggle-js lh:normal p:0_5px c:color-gray9 c:color-primary|h trs:0.3s"
            data-css="{{ action.VEDA_styleBox_8 }}"
          >
            <div class="pos:relative d:inline-block">
              <i class="far fa-heart"></i>
              <div
                class="veda-wishlist-badge-js lh:normal d:none! jc:center ai:center c:color-light-freeze pos:absolute t:-4px r:-8px miw:16px h:16px fz:9px bdrs:8px bgc:#eb4747 fw:500 pl:2px pr:2px"
                data-css="{{ action.VEDA_styleBox_10 }}"
              ></div>
            </div>
          </a>
        {% elsif block.type == 'right_actions_compare' %}
          <div
            class="veda-compare-toggle-js p:0_5px c:color-gray9 c:color-primary|h trs:0.3s cur:pointer"
            data-css="{{ action.VEDA_styleBox_8 }}"
            data-options="
              {
                compareOptionsTitle: '{{ action.VEDA_tags_1 }}',
              }
            "
          >
            <div class="pos:relative d:inline-block">
              <i class="far fa-repeat"></i>
              <div
                class="veda-compare-badge-js d:none! jc:center ai:center c:color-light-freeze pos:absolute t:-4px r:-8px miw:16px h:16px fz:9px bdrs:8px bgc:#eb4747 fw:500 pl:2px pr:2px"
                data-css="{{ action.VEDA_styleBox_10 }}"
              ></div>
            </div>
          </div>
        {% elsif block.type == 'right_actions_search' %}
          <span
            data-css="{{ action.VEDA_styleBox_8 }}"
            class="btn-search-js p:0_5px cur:pointer c:color-gray9 c:color-primary|h trs:0.3s"
          >
            <i class="far fa-search"></i>
          </span>
        {% elsif block.type == 'right_actions_link' %}
          <a
            href="{{ action.right_actions___link }}"
            {% if action.right_actions___new_tab %}
              target="_blank"
            {% endif %}
            data-css="{{ action.VEDA_styleBox_8 }}"
            class="p:0_5px cur:pointer c:color-gray9 c:color-primary|h trs:0.3s"
          >
            {{ action.right_actions___icon }}
          </a>
        {% elsif block.type == 'right_actions_space' %}
          <div
            class="d:flex jc:center"
            style="width: {{ action.VEDA_slider_6 }}px; height: {{ action.VEDA_slider_7 }}px"
          >
            {% if action.VEDA_select_4 == 'line' %}
              <div class="h:100% w:1px" style="background-color: {{ action.VEDA_color_4 }}"></div>
            {% endif %}
          </div>
        {% endif %}
      {% endif %}
    {% endfor %}

    {% capture addon_id_e84d7c12-e9a5-479a-b794-5520774c07ba_content %}
      {% render 'veda-addon-id_e84d7c12-e9a5-479a-b794-5520774c07ba' %}
    {% endcapture %}

    {% unless addon_id_e84d7c12-e9a5-479a-b794-5520774c07ba_content contains 'Could not find asset' %}
      {% render 'veda-addon-id_e84d7c12-e9a5-479a-b794-5520774c07ba',
        uniqueId: uniqueId,
        builderMode: builderMode,
        envatoMode: envatoMode,
        backgroundFixed: backgroundFixed,
        _prefix: _prefix,
        pl: pl,
        action: action
      %}
    {% endunless %}
  </div>
{% endcapture %}

{% capture LanguageAndCurrency %}
  {% for block in section.blocks %}
    {% assign lcItem = block.settings %}

    {% if lcItem.VEDA_switch_4 %}
      {% if block.type == 'VEDA_array_1' %}
        <div
          {% if builderMode %}
            data-tooltip-position="right"
            data-tooltip-text="When you use the editor this data will be fake"
          {% endif %}
        >
          {% liquid
            assign localization_id = 'localization_form_' | append: uniqueId
          %}
          {% form 'localization', id: localization_id, class: 'localization_form' %}
            <input type="hidden" name="locale_code" value="{{ form.current_locale.iso_code }}">
            <div class="veda-select veda-select-lang">
              <div
                data-css="{{ lcItem.VEDA_styleBox_12 }}"
                class="veda-select__view c:color-gray8 d:flex ai:center jc:space-between bd:0! p:5px_0! fz:13px ff:font-secondary"
              >
                <div class="veda-select__label"></div>
                <span class="pt:2px pl:4px"><i class="far fa-angle-down"></i></span>
              </div>
              <div
                data-css="{{ lcItem.VEDA_styleBox_14 }}"
                class="veda-select__options bgc:color-light w:fit-content! bxsh:0_5px_15px_color-gray9.2 bdrs:5px ov:hidden bd:0! p:10px_0"
              >
                {% for language in localization.available_languages %}
                  {% liquid
                    assign defaultClass = 'veda-select__option whs:nowrap bd:0! ff:font-secondary fz:13px p:5px_15px bgc:transparent trs:0.3s'
                  %}
                  <div
                    data-value="{{ language.iso_code }}"
                    {% if language.iso_code == localization.language.iso_code %}
                      {% if builderMode %}
                        class="{{ defaultClass }}"
                      {% else %}
                        class="{{ defaultClass }} c:color-primary"
                      {% endif %}
                      data-selected="selected"
                    {% else %}
                      class="{{ defaultClass }} c:color-primary|h"
                    {% endif %}
                  >
                    {{ language.endonym_name }}
                  </div>
                {% endfor %}
              </div>
            </div>
          {% endform %}
        </div>
      {% elsif block.type == 'VEDA_array_2' %}
        {% liquid
          assign currencySize = shop.enabled_currencies | size
        %}
        {% if currencySize > 1 %}
          <div
            {% if builderMode %}
              data-tooltip-position="right"
              data-tooltip-text="When you use the editor this data will be fake"
            {% endif %}
          >
            {% liquid
              assign currency_id = 'currency_form_' | append: uniqueId
            %}
            {% form 'localization', id: currency_id, class: 'currency_form' %}
              <input
                type="hidden"
                name="currency_code"
                value="{{ form.current_currency.iso_code }}"
              >
              <div class="veda-select veda-select-currency">
                <div
                  data-css="{{ lcItem.VEDA_styleBox_12 }}"
                  class="veda-select__view c:color-gray8 d:flex ai:center jc:space-between bd:0! p:5px_0! fz:13px ff:font-secondary"
                >
                  {% if builderMode %}
                    <span class="mr:5px flag flag-USD"></span>
                    <div class="veda-select__label">USD</div>
                  {% else %}
                    <span class="mr:5px flag flag-{{ form.current_currency.iso_code }}"></span>
                    <div class="veda-select__label">{{ form.current_currency.iso_code }}</div>
                  {% endif %}
                  <span class="pt:2px pl:4px"><i class="far fa-angle-down"></i></span>
                </div>
                <div
                  data-css="{{ lcItem.VEDA_styleBox_14 }}"
                  class="veda-select__options bgc:color-light w:fit-content! bxsh:0_5px_15px_color-gray9.2 bdrs:5px ov:hidden bd:0! p:10px_0"
                >
                  {% for currency in shop.enabled_currencies %}
                    <div
                      data-value="{{ currency.iso_code }}"
                      {% if form.current_currency.iso_code == currency.iso_code %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:13px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary"
                        data-selected="selected"
                      {% elsif builderMode and currency.iso_code == 'USD' %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:13px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary|h"
                        data-selected="selected"
                      {% else %}
                        class="veda-select__option whs:nowrap bd:0! ff:font-secondary fz:13px p:5px_15px d:flex ai:center bgc:transparent trs:0.3s c:color-primary|h"
                      {% endif %}
                    >
                      <span class="mr:5px flag flag-{{ currency.iso_code }}"></span>
                      {{ currency.iso_code }}
                    </div>
                  {% endfor %}
                </div>
              </div>
            {% endform %}
          </div>
        {% endif %}
      {% elsif block.type == 'VEDA_array_3' %}
        <a
          class="c:color-light-freeze"
          data-css="{{ lcItem.VEDA_styleBox_16 }}"
          href="{{ lcItem.VEDA_linkPicker_1 }}"
        >
          {% if lcItem.VEDA_switch_5 %}
            <span class="mr:5px">{{ lcItem.VEDA_icon_2 }}</span>
          {% endif %}
          <span>{{ lcItem.VEDA_text_1 }}</span>
        </a>
      {% elsif block.type == 'VEDA_array_4' %}
        <div
          class="d:flex jc:center"
          style="width: {{ lcItem.VEDA_slider_8 }}px; height: {{ lcItem.VEDA_slider_9 }}px"
        >
          {% if lcItem.VEDA_select_5 == 'line' %}

{% capture Topbar %}
  <div data-css="{{ section.settings["topbar_design__id"] }}" class="bdb:1px_solid_color-gray2">
    <div
      class="pl:15px@+md! pr:15px@+md! {{ containerType }}"
      {% if section.settings.VEDA_slider_1
        or section.settings.VEDA_slider_1 == 0
        or section.settings.section_settings___width
        or section.settings.section_settings___width == 0
      %}
        style="{{ containerGap }}{{ containerWidth }}"
      {% endif %}
    >
      <div class="pos:relative d:flex ai:center jc:space-between pt:10px pb:10px">
        <div class="d:flex ai:center w:33.3333%@md">
          {{ LanguageAndCurrency }}
        </div>
        <div
          class="d:none@+md ta:center c:color-gray9 c:color-primary*a ff:font-secondary w:33.3333%@md"
          data-css="{{ section.settings["VEDA_styleBox_6"] }}"
        >
          {{ section.settings.announcement_bar___text }}
        </div>
        <div class="social-js w:33.3333%@md d:flex ai:center jc:flex-end">
          <div class="d:inline-block"></div>

          {% for block in section.blocks %}
            {% if block.type == 'socials' %}
              {% assign social = block.settings %}

              <a
                class="p:4px_6px c:color-gray9 c:color-gray7|h trs:0.3s"
                data-css="{{ social.socials___design__id }}"
                target="_blank"
                href="{{ social.socials___link }}"
                data-tooltip-text="{{ social.socials___label }}"
                data-tooltip-position="bottom"
                style="--tooltip-radius: 5px"
              >
                <span class="fz:16px">{{ social.socials___icon }}</span>
              </a>
            {% endif %}
          {% endfor %}
          <div class="d:inline-block"></div>
        </div>
      </div>
    </div>
  </div>
{% endcapture %}

{% capture Content %}
  <div
    {% if section.settings.navigation___sticky %}
      class="veda-sticky w:100%"
    {% endif %}
  >
    <div class="w:100% bgc:inherit">
      <div
        class="pl:15px@+md! pr:15px@+md! {{ containerType }}"
        {% if section.settings.VEDA_slider_1
          or section.settings.VEDA_slider_1 == 0
          or section.settings.section_settings___width
          or section.settings.section_settings___width == 0
        %}
          style="{{ containerGap }}{{ containerWidth }}"
        {% endif %}
      >
        <nav class="veda-nav d:flex ai:center jc:space-between mih:60px">
          <div class="d:flex ai:center">
            <div
              data-css="{{ section.settings["hamburger___design__id"] }}"
              class="veda-nav__hamburger veda-hamburger-menu-icon fz:20px c:color-gray9 mr:20px d:none"
            >
              {{ section.settings.hamburger___icon }}
            </div>
            {{ Logo }}
            {{ Navigation }}
          </div>
          {{ Actions }}
        </nav>
      </div>
      <div class="h:1px bgc:color-gray2"></div>
    </div>
  </div>
{% endcapture %}

<header
  data-id="{{ uniqueId }}"
  class="pos:relative z:99"
>
  <div
    class="veda-background veda-lazyload pos:absolute ins:0 z:-1 bgz:cover bgp:center bgc:color-light"
    style="{{ backgroundColor }}{{ backgroundImage }}{{ sectionPadding }}{{ sectionSeparator }}"
  ></div>
  <div>
    {{ Topbar }}
    {{ Content }}
  </div>
  {% comment %}BAT_DAU_TACH_FILE{% endcomment %}
  <div class="search-content-js pos:fixed t:0 l:0 z:999999 w:100% bgc:color-light trs:0.3s trf:translateY(-100%) v:hidden d:flex jc:center">
    <span class="btn-search-js pos:absolute t:10px r:20px fz:25px c:color-gray6 c:color-gray9|h cur:pointer trs:0.3s">
      <i class="fal fa-times"></i>
    </span>
    <div
      class="veda-search-form-js w:100% h:100% p:30px_15px d:flex jc:center"
      data-text-result="{{ 'veda.templates.search.search_for' | t: terms: 'a' }}"
    >
      <form
        data-css="{{ section.settings["VEDA_styleBox_18"] }}"
        action="{{ routes.search_url }}"
        method="GET"
        class="d:flex ai:center jc:space-between w:80% maw:800px bdb:3px_solid_color-gray3"
      >
        <div class="d:flex ai:center w:100%">
          <input
            data-css="{{ section.settings["VEDA_styleBox_20"] }}"
            class="input-search-js rs w:100% bd:0! bxsh:none! o:none! bgc:transparent ff:font-secondary fz:pfs(26px,34px) c:color-gray8 c:color-gray5||ph trs:0.3s"
            type="text"
            placeholder="{{ 'veda.general.search.search' | t }}"
            name="q"
            value="{{ search.terms | escape }}"
            autocorrect="off"
            autocomplete="off"
          >
          <div
            class="veda-spinner bdw:2px mr:10px d:none"
            style="--spinner-color: var(--color-primary); --spinner-size: 14px;display: none"
          ></div>
          {% if section.settings.veda_search___type %}
            <input type="hidden" name="type" value="{{ section.settings["veda_search___type"] | replace: ' ', '' }}">
          {% else %}
            <input type="hidden" name="type" value="product,article,collection,page">
          {% endif %}
          {% if section.settings.VEDA_select_6 %}
            <input type="hidden" name="options[unavailable_products]" value="last">
          {% else %}
            <input type="hidden" name="options[unavailable_products]" value="{{ section.settings["VEDA_select_6"] }}">
          {% endif %}
          {% if _prefix %}
            <input type="hidden" name="options[prefix]" value="{{ _prefix }}">
          {% else %}
            <input type="hidden" name="options[prefix]" value="last">
          {% endif %}
          {% if section.settings.veda_search___fields %}
            <input type="hidden" name="options[fields]" value="{{ section.settings["veda_search___fields"] }}">
          {% else %}
            <input type="hidden" name="options[fields]" value="title,vendor,product_type,variants.title">
          {% endif %}
          {% if section.settings.VEDA_slider_10 %}
            <input type="hidden" name="limit" value="{{ section.settings["VEDA_slider_10"] }}">
          {% else %}
            <input type="hidden" name="limit" value="10">
          {% endif %}
        </div>
        <button
          data-css="{{ section.settings["VEDA_styleBox_22"] }}"
          class="rs flxs:0 bxsh:none bd:0 bgc:transparent! bxsh:none! o:none! fz:pfs(18px,26px) c:color-gray5 c:color-primary|h trs:0.3s cur:pointer"
          type="submit"
          aria-label="Search"
        >
          <i class="far fa-search"></i>
        </button>
      </form>
    </div>
  </div>
  <div class="h:1px bgc:color-gray2"></div>
  {% comment %}KET_THUC_TACH_FILE{% endcomment %}

  {% if section.settings.VEDA_select_2 == 'video' and section.settings.VEDA_video_1 %}
    <div data-veda-video="{{ section.settings["VEDA_video_1"] }}"></div>
  {% endif %}
  {% if section.settings.VEDA_switch_2 %}
    <div
      class="pos:absolute ins:0 z:-1"
      style="background-color: {{ section.settings["VEDA_color_2"] }};backdrop-filter: blur({{ section.settings["VEDA_slider_3"] }}px)"
    ></div>
  {% endif %}
</header>
